<p id="notice"><%= notice %></p>

<h1>Listing Process Type Inclusions</h1>

<table>
  <thead>
    <tr>
      <th>Process type</th>
      <th>Item class</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @process_type_inclusions.each do |process_type_inclusion| %>
      <tr>
        <td><%= process_type_inclusion.process_type %></td>
        <td><%= process_type_inclusion.item_class %></td>
        <td><%= link_to 'Show', process_type_inclusion %></td>
        <td><%= link_to 'Edit', edit_process_type_inclusion_path(process_type_inclusion) %></td>
        <td><%= link_to 'Destroy', process_type_inclusion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Process type inclusion', new_process_type_inclusion_path %>
