<p id="notice"><%= notice %></p>

<h1>Listing Process Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @process_types.each do |process_type| %>
      <tr>
        <td><%= process_type.name %></td>
        <td><%= process_type.description %></td>
        <td><%= link_to 'Show', process_type %></td>
        <td><%= link_to 'Edit', edit_process_type_path(process_type) %></td>
        <td><%= link_to 'Destroy', process_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Process type', new_process_type_path %>
